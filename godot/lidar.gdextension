[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://../rust/target/debug/liblidar.so"
linux.release.x86_64 =   "res://../rust/target/release/liblidar.so"
windows.debug.x86_64 =   "res://../rust/target/debug/lidar.dll"
windows.release.x86_64 = "res://../rust/target/release/lidar.dll"
macos.debug =            "res://../rust/target/debug/liblidar.dylib"
macos.release =          "res://../rust/target/release/liblidar.dylib"git pull
macos.debug.arm64 =      "res://../rust/target/debug/liblidar.dylib"
macos.release.arm64 =    "res://../rust/target/release/liblidar.dylib"
